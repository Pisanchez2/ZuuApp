<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ecualac.zuuapp.ui.supervisores.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home_sup" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.ecualac.zuuapp.ui.supervisores.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard_sup" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.ecualac.zuuapp.ui.supervisores.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications_sup" />
</navigation>